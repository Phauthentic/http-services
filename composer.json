{
    "name": "phauthentic/http-serices",
    "description": "Services around PSR7 HTTP Messages",
    "type": "library",
    "require": {
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "php": "^7.1",
        "phpro/grumphp": "^0.14.3",
        "phpstan/phpstan": "^0.10.8",
        "phpunit/phpunit": "^7.5",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "Phauthentic\\Http\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Phauthentic\\Http\\Test\\": "tests/TestCase/"
      }
    },
    "config": {
      "sort-packages": true
    }
}
